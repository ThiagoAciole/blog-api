{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/posts",
      "dest": "/routes/posts.js"
    },
    {
      "src": "/api-docs",
      "dest": "/routes/posts.js"
    },
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
