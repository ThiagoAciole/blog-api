{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/posts/(.*)",
      "dest": "/routes/posts.js"
    },
    {
      "src": "/api-docs/(.*)",
      "dest": "/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "/app.js"
    }
  ],
  "functions": {
    "api/*": {
      "maxDuration": 30 
    }
  }
}
